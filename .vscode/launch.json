  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch AppEngine",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/AppEngine/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch ComputeEngine-Updater",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/ComputeEngine-Updater/dist/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch CloudFunctions",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/CloudFunctions/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    }
  ]
}
