  "version": "0.2.0",
  "configurations": [
    {
      "name": "AppEngine",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/AppEngine/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "name": "AppEngine - Test OpenDataVehicleProvider",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/AppEngine/dist/test-open-data-vehicle-provider.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "name": "AppEngine - Test MpkVehicleProvider",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/AppEngine/dist/test-mpk-vehicle-provider.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "name": "ComputeEngine-Updater",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/ComputeEngine-Updater/dist/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "name": "ComputeEngine-Notifications",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/ComputeEngine-Notifications/dist/app.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "name": "CloudFunctions",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/CloudFunctions/index.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ]
    },
    {
      "name": "Python: siege",
      "type": "python",
      "request": "launch",
      "program": "./siege/run.py"
    },
  ]
}
